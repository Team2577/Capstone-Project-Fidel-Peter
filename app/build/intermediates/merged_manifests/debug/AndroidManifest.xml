<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.capstoneproject.fidel_peter"
    android:versionCode="200"
    android:versionName="2.0.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="30" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@drawable/logo_fidel_peter"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_fidel_peter"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.CapstoneProject.NoActionBar" >
        <activity android:name="com.capstoneproject.fidel_peter.fidel_peter_CuisineActivity" />
        <activity android:name="com.capstoneproject.fidel_peter.fidel_peter_DishActivity" />
        <activity android:name="com.capstoneproject.fidel_peter.fidel_peter_OrderDetail" />
        <activity android:name="com.capstoneproject.fidel_peter.fidel_peter_OrderActivity" />
        <activity android:name="com.capstoneproject.fidel_peter.fidel_peter_Activity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>